    {
      "columnName": "INFECTION_ID",
      "ordinalPosition": 1
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "INFECTION_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique identifier for the infection record."
    },
    {
      "ordinalPosition": 2,
      "name": "RECORD_STATUS_C_NAME",
      "type": "VARCHAR",
      "discontinued": false,
      "description": "The record status category ID for the infection.",
      "entries": [
        "Soft Deleted",
        "Hidden",
        "Hidden and Soft Deleted"
      ]
