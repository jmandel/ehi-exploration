{
  "name": "ORD_MED_PRNREASONS",
  "description": "This table is to show PRN reasons for all PRN medication orders. If you want to get more information about a medication order, you could link this table to ORDER_MED or ORDER_MEDINFO.",
  "primaryKey": [
    {
      "columnName": "ORDER_MED_ID",
      "ordinalPosition": 1
    },
    {
      "columnName": "LINE",
      "ordinalPosition": 2
    }
  ],
  "columns": [
    {
      "ordinalPosition": 1,
      "name": "ORDER_MED_ID",
      "type": "NUMERIC",
      "discontinued": false,
      "description": "The unique ID of the medication order (prescription) record. NOTE: This is an internal unique identifier for order records in this table and cannot be used to link to CLARITY_MEDICATION."
    },
    {
      "ordinalPosition": 2,
